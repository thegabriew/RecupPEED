import random
 
inicio = 1   
fim = 10    
 
n = 1     
 
resposta = random.randint(inicio,fim)

while True:
    
    palpite = int(input("Insira um nÃºmero entre 1 e 50: "))

    if palpite > resposta:
        print("Palpite muito alto, tente novamente")
    elif palpite < resposta:
        print("Palpite muito baixo, tente novamente")
    else:
        print("Resposta certa.")
        break
